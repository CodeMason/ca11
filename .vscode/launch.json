{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Launch to Process",
      "port": 9229,
      "protocol": "inspector"
    },
    {
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "name": "Attach Chrome",
      "url": "https://localhost:3000",
      "webRoot": "${workspaceFolder}/build/ca11/webview",
      "sourceMaps": true,
      "trace": true,
      "sourceMapPathOverrides": {
        "*": "${workspaceRoot}/*"
      }
    }
  ]
}
